import { ModuleWithProviders } from '@angular/core';
import { AutofocusFixConfig, AutofocusFixOptions } from './autofocus-fix-config';
export declare function configFactory(options: AutofocusFixOptions): AutofocusFixConfig;
export declare class AutofocusFixModule {
    constructor($config: AutofocusFixConfig);
    static forRoot(options?: AutofocusFixOptions): ModuleWithProviders;
}
